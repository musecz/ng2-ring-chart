{
  "name": "ng2-ring-chart",
  "version": "1.1.3",
  "description": "A simple way to make a ring chart",
  "main": "index.js",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ringChart.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ringChart.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "keywords": [
    "ring",
    "chart"
  ],
  "author": "schnouz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/musecz/ng2-ring-chart.git"
  },
  "homepage": "https://github.com/musecz/ng2-ring-chart",
  "bugs": {
    "url": "https://github.com/musecz/ng2-ring-chart.git/issues"
  },
  "devDependencies": {
    "@angular/compiler": "^2.4.8",
    "@angular/compiler-cli": "^2.4.8",
    "rollup": "^0.41.4",
    "typescript": "^2.2.1",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/core": "^2.4.8",
    "rxjs": "^5.2.0"
  }
}